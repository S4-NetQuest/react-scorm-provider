{
  "name": "react-scorm-provider",
  "version": "0.2.1",
  "description": "Components to easily enable SCORM API communication in React projects.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production",
    "test": "jest --verbose"
  },
  "keywords": [
    "react",
    "scorm",
    "scorm1.2",
    "scorm2004",
    "lms",
    "sco",
    "elearning",
    "e-learning",
    "wrapper",
    "api"
  ],
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/tests/setupTests.js"
    ]
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "pipwerks-scorm-api-wrapper": "^0.1.2",
    "prop-types": "^15.7.2",
    "react-autobind": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.8.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-async-helpers": "^0.9.1",
    "enzyme-cleanup": "^1.1.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.13.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "author": "S4 NetQuest",
  "homepage": "https://github.com/S4-NetQuest/react-scorm-provider",
  "repository": {
    "type": "git",
    "url": "git@github.com:S4-NetQuest/react-scorm-provider.git"
  }
}
